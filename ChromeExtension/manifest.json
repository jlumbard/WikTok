{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": ["tabs","webRequest","webNavigation","activeTab","declarativeContent", "storage","cookies","http://localhost/","http://127.0.0.1:3000/","http://127.0.0.1/","http://localhost:5000/","https://en.wikipedia.org/*"],
  "options_page": "options.html",
  "web_accessible_resources": [
    "https://127.0.0.1:5000/insert",
    "navigation.html"
  ],
  "content_scripts": [
    {
      "matches": ["https://en.wikipedia.org/*"],
      "js": ["content-script.js"]
    }
  ],
  "background":{
    "scripts":["background.js"],
    "persistent":true
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },

  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
